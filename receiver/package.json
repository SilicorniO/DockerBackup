{
  "name": "DockerBackupReceiver",
  "version": "0.1.0",
  "description": "Docker Backup Receiver",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "start": "webpack --config webpack/webpack.local.js && node --inspect=0.0.0.0:9400 dist/server.js",
    "start:watch": "webpack --config webpack/webpack.local.js && nodemon --watch 'src/**/*.ts' --inspect=0.0.0.0:9400 dist/server.js",
    "build-local": "webpack --config webpack/webpack.local.js",
    "build-pro": "webpack --config webpack/webpack.pro.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix src --ext .js,.jsx,.ts,.tsx",
    "prettier:fix": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "strings": "node update_strings.js"
  },
  "author": "Javier S.R. SilicorniO",
  "license": "MIT",
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.17.1",
    "inversify": "5.0.5",
    "moment": "2.29.1",
    "moment-timezone": "0.5.32",
    "multer": "1.4.2",
    "mysql2": "2.2.5",
    "node-fetch": "2.6.1",
    "reflect-metadata": "0.1.13",
    "request": "2.88.2",
    "rxjs": "6.6.3",
    "source-map-loader": "2.0.0"
  },
  "devDependencies": {
    "@types/express": "4.17.9",
    "@types/jest": "26.0.19",
    "@types/moment-timezone": "0.5.30",
    "@types/multer": "1.4.5",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "14.14.16",
    "@types/rx": "4.1.2",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "awesome-typescript-loader": "5.2.1",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "7.0.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.0",
    "jest": "26.6.3",
    "node-optimize": "1.2.3",
    "node-schedule": "1.3.2",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.12",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "webpack": "5.11.0",
    "webpack-cli": "4.2.0",
    "webpack-filter-warnings-plugin": "1.2.1",
    "webpack-merge": "5.7.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".*\\.test.(ts|tsx|js)$"
  }
}
